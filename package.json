    "vue-loader": "^15.2.0",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.0.2",
    "vue-template-compiler": "^2.5.9",
    "vuex": "^3.0.1",
    "webpack": "^4.16.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.0",
    "webpackbar": "^4.0.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 7",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
